<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="peson" class="com.sakii209.spring_continerbihaver_04_09_19.Person">
<property name="name" value="saikiran" />
<property name="pid" value="209" />
<property name="salary" value="20000" />
<property name="address" ref="address" />
</bean>

<bean id="address" class="com.sakii209.spring_continerbihaver_04_09_19.Address" scope="prototype">
<property name="city" value="hyd" />
<property name="state" value="TS" />
<property name="street" value="Bulkampet" />
</bean>

</beans>
